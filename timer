<script>
window.onload=oppstart;

function oppstart(){
	var naa = new Date();
    var eksamen = new Date("12/05/2017 09:00");

    var differanse= eksamen.getTime()-naa.getTime();
    var sekunder=differanse/1000;
    var timer= sekunder/3600;

    document.getElementById("utskrift").innerHTML=sekunder+"sekunder":
	//5/12-2017 kl09.00 eksamen
}


